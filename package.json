{
  "name": "react-shortcut",
  "version": "1.0.0",
  "description": "a full react shortcut solution",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heychenfq/react-shortcut.git"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsc -b -w",
    "build": "rimraf dist && tsc -b",
    "test": "jest",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "shortcut"
  ],
  "author": "chenfuqiang",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "husky": "^8.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "dependencies": {
    "debug": "^4.3.4"
  }
}
